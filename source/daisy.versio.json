{
	"max_apps":1,
	"defines": {
		"OOPSY_TARGET_VERSIO": 1
	},
	"labels": {
		"params": {
			"knob1": "kn1",
			"knob2": "kn2",
			"knob3": "kn3",
			"knob4": "kn4",
			"knob5": "kn5",
			"knob6": "kn6",
			"knob7": "kn7",
			"button1": "bt1",
			"sw1": "sw1",
			"sw2": "sw2",
			"gate1": "gt1",

			"cv1": "kn1",
			"cv2": "kn2",
			"cv3": "kn3",
			"cv4": "kn4",
			"cv5": "kn5",
			"cv6": "kn6",
			"cv7": "kn7",
			"ctrl1": "kn1",
			"ctrl2": "kn2",
			"ctrl3": "kn3",
			"ctrl4": "kn4",
			"ctrl5": "kn5",
			"ctrl6": "kn6",
			"ctrl7": "kn7",
			"switch1": "sw1",
			"switch2": "sw2",

			"knob": "kn1",
			"button": "bt1",
			"ctrl": "kn1",
			"cv": "kn1",
			"switch": "sw1",
			"gate": "gt1",

			"tap": "bt1",
			"fsu": "bt1",
			"abc": "sw1",
			"xyz": "sw2"			
		},
		"outs": {
			"led1": "led1",
			"led2": "led2",
			"led3": "led3",
			"led4": "led4"
		},
		"datas": {}
	},
	"inputs": {
		"kn1": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_0)"
		},
		"kn2": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_1)"
		},
		"kn3": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_2)"
		},
		"kn4": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_3)"
		},
		"kn5": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_4)"
		},
		"kn6": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_5)"
		},
		"kn7": {
			"automap": true,
        	"getter": "hardware.GetKnobValue(hardware.KNOB_6)"
		},
		"sw1": {
			"getter": "(hardware.sw[0].Read()/2.f)"
		},
		"sw2": {
			"getter": "(hardware.sw[1].Read()/2.f)"
		},
		"bt1": {
			"getter": "(hardware.tap.Pressed()?1.f:0.f)",
			"update": {
				"where": "audio",
				"code": "hardware.tap.Debounce();"
			}
		},
		"gt1": {
			"getter": "(hardware.Gate()?1.f:0.f)"
		}
	},
	"outputs": {
		"led1": {
			"where": "audio",
			"setter": "hardware.leds[0].Set(clamp(-$<name>, 0.f, 1.f), clamp($<name>, 0.f, 1.f), 0.f)"
		},
		"led2": {
			"where": "audio",
			"setter": "hardware.leds[1].Set(clamp(-$<name>, 0.f, 1.f), clamp($<name>, 0.f, 1.f), 0.f)"
		},
		"led3": {
			"where": "audio",
			"setter": "hardware.leds[2].Set(clamp(-$<name>, 0.f, 1.f), clamp($<name>, 0.f, 1.f), 0.f)"
		},
		"led4": {
			"where": "audio",
			"setter": "hardware.leds[3].Set(clamp(-$<name>, 0.f, 1.f), clamp($<name>, 0.f, 1.f), 0.f)"
		}
	},
    "datahandlers": {},
	"audio_ins": [
      "dsy_in1",
      "dsy_in2"
    ],
    "audio_outs": [
      "dsy_out1",
      "dsy_out2"
    ],
    "midi_ins": [],
    "midi_outs": []
}